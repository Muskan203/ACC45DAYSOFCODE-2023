#include <iostream>

int main() {
    int T;
    std::cin >> T;

    while (T--) {
        long long X, Y; // Use long long to handle large values
        std::cin >> X >> Y;

        if (X >= Y) {
            std::cout << "0" << std::endl; // Chef should not rent the cooler
        } else {
            long long months = Y / X;
            std::cout << months << std::endl;
        }
    }

    return 0;
}
